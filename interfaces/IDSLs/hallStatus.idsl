module hallStatusComp
{
	struct PlayerPos
	{
		 float x;
		 float y;
		 float z;
	};

	struct PersonInfo
	{
		 PlayerPos pos;
		 bool predicted;
		 PlayerPos vol;
		 long id;
		 int idCam;
	};
	sequence <PersonInfo> People;

	struct hallData
	{
		 People data;
	};
	interface peopleHall
	{
		idempotent hallData getData ();
	};
};
};
